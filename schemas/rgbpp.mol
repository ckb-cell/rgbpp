import blockchain;

# RGBPP config
struct RGBPPConfig {
  # Type hash of bitcoin light client
  btc_lc_type_hash: Byte32,
  # Type hash of bitcoin time lock contract
  btc_time_lock_type_hash: Byte32,
}

# RGBPP lock args
struct RGBPPLock {
  out_index: Uint32,
  btc_txid: Byte32,
}

# RGBPP unlock witness
table RGBPPUnlock {
  btc_tx: Bytes,
}

# BTCTimeLock

struct BTCTimeLock {
  lock_hash: Byte32,
  after: Uint32,
  btc_txid: Byte32,
}

